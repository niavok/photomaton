sudo chmod -x /usr/lib/gvfs/gvfs-gphoto2-volume-monitor

sudo adduser www-data plugdev

sudo service apache2 restart

mkdir -p /home/fred/photomaton/
chmod 777 /home/fred/photomaton

mkdir -p /home/fred/store/photomaton/
chmod 777 /home/fred/store/photomaton/


lpr -o page-top=0 -o page-left=0 -o page-right=0
 -o page-bottom=0 -o page-border=none -o position=top -o media=Photo10x15 -o fit-to-page -o scaling=100 DSC_9114.JPG


convert DSC_9114.JPG DSC_9114.pdf
lpr DSC_9114.pdf
